MaterialResource:
  common:
    references:
      []
  baseType: Shader
  basePath: internal\shaders\BaseSurfacePBR.wgsl
  domain:
    type: Surface
    usage: StaticMesh|SkeletalMesh
    shadingModel: Lit
    blendMode: Opaque
  definitions:
    - first: MATERIAL_SCROLL_SPEED_U
      second:
        index: 2
        data: 0.5
    - first: MATERIAL_SCROLL_SPEED_V
      second:
        index: 2
        data: 1
    - first: MATERIAL_ENABLE_ARBITRARY_FLAG
      second:
        index: 3
        data: true
  samplers:
    - first: surfaceSampler
      second:
        addressModeU: ClampToEdge
        addressModeV: ClampToEdge
        addressModeW: ClampToEdge
        magFilter: Nearest
        minFilter: Nearest
        mipmapFilter: Nearest
        lodMinClamp: 0
        lodMaxClamp: 1000
        compare: Undefined
        maxAnisotropy: 1
  textures:
    - first: albedo
      second: c\c\T_A.png
    - first: normal
      second: c\c\T_B.png
    - first: roughness
      second: c\c\T_C.png
  uniforms:
    - first: packetParams.modelMatrix
      second:
        index: 9
        data:
          value0:
            value0: 1
            value1: 0
            value2: 0
            value3: 0
          value1:
            value0: 0
            value1: 1
            value2: 0
            value3: 0
          value2:
            value0: 0
            value1: 0
            value2: 1
            value3: 0
          value3:
            value0: 0
            value1: 0
            value2: 0
            value3: 1
    - first: materialParams.tint
      second:
        index: 8
        data:
          value0: 1
          value1: 0.5
          value2: 0.5
          value3: 0.5